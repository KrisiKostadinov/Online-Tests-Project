<div class="container">
    <div class="row mt-5">
        {{> error-messages }}
        {{#each tests }}
        <div class="col-md-4">
            <div class="list-group-item p-3 mb-4 dcs d-flex justify-content-between" id="{{ _id }}">
                <a href="/tests/details/{{_id}}" class="nav-link custom-link">{{ title }}</a>
            </div>
        </div>
        {{/each }}
    </div>
</div>